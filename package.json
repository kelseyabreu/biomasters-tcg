{
  "name": "biomasters-tcg",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "workspaces": [
    "packages/shared",
    "packages/server"
  ],
  "scripts": {
    "build:shared": "npm run build --workspace=packages/shared",
    "build:server": "npm run build --workspace=packages/server",
    "build:frontend": "tsc && vite build",
    "build:all": "npm run build:shared && npm run build:server && npm run build:frontend",
    "dev:shared": "npm run dev --workspace=packages/shared",
    "dev:server": "npm run dev --workspace=packages/server",
    "dev": "vite",
    "build": "npm run build:shared && tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "test.e2e": "playwright test",
    "test.e2e.ui": "playwright test --ui",
    "test.e2e.headed": "playwright test --headed",
    "test.e2e.auth": "playwright test e2e/auth-and-deletion.spec.ts",
    "test.e2e.auth.headed": "playwright test e2e/auth-and-deletion.spec.ts --headed",
    "test.unit": "vitest",
    "firebase:emulators": "firebase emulators:start --only auth",
    "firebase:emulators.ui": "firebase emulators:start --only auth,ui",
    "lint": "eslint",
    "android:build": "npm run build:prod && npx cap sync android && npx cap build android",
    "android:open": "npx cap open android",
    "android:run": "npm run build:prod && npx cap sync android && npx cap run android",
    "icons:generate": "npx @capacitor/assets generate"
  },
  "dependencies": {
    "@capacitor/app": "^7.0.1",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/core": "7.4.2",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@tanstack/react-query": "^5.85.5",
    "@types/crypto-js": "^4.2.2",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.12.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "ionicons": "^7.4.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.2",
    "@playwright/test": "^1.55.0",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "eslint": "^9.20.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.24.0",
    "vite": "~5.2.0",
    "vitest": "^0.34.6"
  },
  "overrides": {
    "rollup": "4.44.0"
  },
  "description": "An Ionic project"
}
