{
  "name": "@kelseyabreu/server",
  "version": "1.0.0",
  "description": "Backend API server for Biomasters TCG",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npx tsc",
    "start": "node dist/index.js",
    "test:memorystore": "tsx src/scripts/test-memorystore.ts",
    "test:redis-cloud": "tsx src/scripts/test-redis-cloud.ts",
    "test": "jest",
    "test:matchmaking": "jest --testPathPattern=matchmaking",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:e2e:1v1": "jest --testPathPattern=e2e.*1v1",
    "test:e2e:2v2": "jest --testPathPattern=e2e.*2v2",
    "test:e2e:ffa": "jest --testPathPattern=e2e.*ffa",
    "db:migrate": "tsx src/database/migrator.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:reset": "tsx src/scripts/reset.ts",
    "db:reset-and-seed": "npm run db:reset && npm run db:migrate && npm run db:seed",
    "db:import-species": "tsx src/scripts/import-species.ts",
    "db:import-biomasters": "tsx src/scripts/import-biomasters-data.ts",
    "test:pubsub": "tsx src/scripts/check-pubsub.ts",
    "setup:pubsub": "tsx src/scripts/setup-pubsub-resources.ts",
    "redis:tunnel": "start-redis-tunnel.bat",
    "deploy:dev": "node scripts/deploy-to-cloud-run.js .env.development --docker",
    "deploy:devNoDocker": "node scripts/deploy-to-cloud-run.js .env.development --cloud-build",
    "deploy:prod": "node scripts/deploy-to-cloud-run.js .env.production --docker",
    "deploy:prodNoDocker": "node scripts/deploy-to-cloud-run.js .env.production --cloud-build"
  },
  "keywords": [
    "tcg",
    "game",
    "api",
    "firebase",
    "express"
  ],
  "author": "Biomasters TCG Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.0",
    "@kelseyabreu/shared": "^1.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "firebase-admin": "^12.0.0",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.5",
    "morgan": "^1.10.0",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.6.13",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^9.0.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.11.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "imports": {
    "@shared/*": "./shared/*"
  }
}
